    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

    <script src="js/bootstrap.bundle.min.js"></script> 
    <script >
      async function btnClick(){
        let obj={}
        let name=document.getElementById("name").value
        let email=document.getElementById("email").value
        let password=document.getElementById("password").value
        let mobile_number=document.getElementById("mobile_number").value
        let invited_by_id=document.getElementById("invited_by_id").value
        let baseUrl=document.getElementById("baseUrl").value
        obj.name=name
        obj.email=email
        obj.password=password
        obj.mobile_number=mobile_number
        obj.invited_by_id=invited_by_id
    

        let response = await axios.post(`http://${baseUrl}/api/wolooGuest/registration`,obj);
        if(response.data.success){
          alert("Registaration Successfully done")

        }
        else{
          alert("Try again")
        }
      }
    </script>    

