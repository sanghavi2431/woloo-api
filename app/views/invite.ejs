                <div class="modal-body">
                  <div class="col-md-12 p-0">
                  	<lable>Name</lable>
                  	<input type="text" id="name" required name="name" class="form-control" />
                  </div>
                  <div class="col-md-12 p-0">
                  	<lable>Email</lable>
                  	<input type="text" id="email" required name="email" class="form-control" />
                  </div>
                 <div class="col-md-12 p-0 mb-2">
                  	<lable>Password</lable>
                  	<input type="text" id="password" required name="password" class="form-control"  pattern="^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[!@#\$&*~]).{8,}$"/>
                  	<span style="font-size:12px;">Note* :- Password must contain one lower case letter, one upper case letter, one number and one speacial character and must be atleast 8 digit</span>
                  </div>
                </div>
                <input type="hidden" id="mobile_number" value="<%= mobile_number %>" name="mobile_number">
                <input type="hidden"  id="invited_by_id" value="<%= invited_by[0].id %>" name="invited_by_id">
                <input type="hidden"  id="baseUrl" value="<%= baseUrl %>" name="baseUrl">

                <div class="modal-footer">
                  <button type="submit" onclick="btnClick()" class="btn btn-info" style="background-color:#fff212;color:#4b4b4d" id="register">Submit</button>
                  <button type="button" class="btn btn-info" data-dismiss="modal" style="background-color:#4b4b4d;color:#fff212">Close</button>
